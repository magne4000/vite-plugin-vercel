{
  "name": "@vite-plugin-vercel/vike",
  "version": "9.0.7",
  "type": "module",
  "files": [
    "dist",
    "templates",
    "*.d.ts",
    "helpers.js",
    "config.js"
  ],
  "description": "Use vike with vite-plugin-vercel",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/vite-plugin-vercel",
  "main": "./dist/vike.js",
  "types": "./dist/vike.d.ts",
  "exports": {
    ".": "./dist/vike.js",
    "./helpers/node": "./dist/templates/node-helpers.js",
    "./helpers/edge": "./dist/templates/edge-helpers.js",
    "./config": {
      "types": "./config.d.ts",
      "default": "./dist/+config.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@types/node": "^18.19.130",
    "@types/qs": "^6.14.0",
    "@vercel/edge": "^1.2.2",
    "@vercel/node": "^3.2.29",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vike": "^0.4.249",
    "vite": "^6.4.1",
    "vite-plugin-vercel": "workspace:*"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
    "nanoid": "^5.1.6",
    "qs": "^6.14.0"
  },
  "peerDependencies": {
    "vike": "^0.4.229",
    "vite": "^5.0.2 || ^6",
    "vite-plugin-vercel": "*"
  },
  "license": "MIT"
}
