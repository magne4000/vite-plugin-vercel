    "@vercel/og": "^0.6.8",
    "@vite-plugin-vercel/vike": "workspace:*",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "cross-fetch": "^4.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vike": "^0.4.249",
    "vite": "^6.4.1",
    "vite-plugin-vercel": "workspace:*"
  },
